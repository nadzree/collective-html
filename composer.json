{
  "name": "nadzree/collective-html",
  "description": "LaravelCollective Html and Form builder for Lumen.",
  "keywords": ["Lumen", "Html", "Form", "Builder"],
  "license": "MIT",
  "authors": [
    {
      "name": "Sudoers Solutions Sdn Bhd",
      "email": "info@sudoers.my"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0",
    "laravel/lumen-framework": "8.*",
    "illuminate/session": "^8.0",
    "illuminate/http": "^8.0",
    "illuminate/support": "^8.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.5.20 || ^8.5.14 || ^9.5.1"
  },
  "replace": {
    "laravelcollective/html": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Collective\\Html\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
